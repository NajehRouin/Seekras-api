{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "outDir": "./dist",
    "rootDir": "./src",
     "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
     "forceConsistentCasingInFileNames": true,
   "types": ["node", "express"]
  },
  "include": [
    "src/**/*.ts"
  ]
}
